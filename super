package com.u8;
class GP
{
	GP()
	{ //super();//self 
		System.out.println("GP class");
	}
}
class P extends GP{
	P()
	{super();
	System.out.println("P class");
	}
}
class c extends P{
	c()
	{super();
	System.out.println("c class");
	}
}
public class superkey {

	public static void main(String[] args) {
		
      c c1=new c(); 
	}

}



output:
GP class
P class
c class
