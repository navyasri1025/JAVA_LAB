package com.u8;
import java.util.*;

public class sum {

	public static void main(String[] args) {
		Scanner read=new Scanner(System.in);
		System.out.print("a=");
		int a=read.nextInt();
		System.out.print("b=");
		int b=read.nextInt();
      System.out.println("sum:"+(a+b));
	}

}
output:
a=9
b=3
sum:12

